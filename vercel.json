{
  "version": 2,
  "builds": [
      {
          "src": "api/main.py",
          "use": "@vercel/python"
      }
  ],
  "rewrites": [
      {
          "source": "/api/(.*)",
          "destination": "/api/main.py"
      },
      {
          "source": "/(.*\\.(ico|png|jpg|jpeg|gif|svg|css|js|webmanifest))",
          "destination": "/project/public/$1"
      },
      {
          "source": "/(.*)",
          "destination": "/project/index.html"
      }
  ]
}